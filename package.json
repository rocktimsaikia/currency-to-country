{
	"name": "my-ts-lib",
	"version": "0.0.0",
	"description": "My awesome typescript library",
	"keywords": [],
	"repository": "rocktimsaikia/",
	"license": "MIT",
	"author": {
		"name": "Rocktim Saikia",
		"email": "hello@rocktimsaikia.dev",
		"url": "https://rocktimsaikia.dev"
	},
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "pnpm build --watch src",
		"prepublishOnly": "pnpm build",
		"test": "vitest run"
	},
	"devDependencies": {
		"@rocktimsaikia/prettier-config": "^1.0.1",
		"prettier": "^2.8.7",
		"tsup": "^6.7.0",
		"typescript": "4.9.5",
		"vitest": "0.28.5"
	},
	"publishConfig": {
		"access": "public"
	},
	"tsup": {
		"entry": [ "src/index.ts" ],
		"format": [ "cjs", "esm" ],
		"clean": true,
		"dts": true
	}
}
